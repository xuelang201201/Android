import java.util.Date;
import java.util.Locale;

public class CommonUtils {
	/**
	 * 格式化日期/时间的工具
	 */
	SimpleDateFormat sdf = new SimpleDateFormat("mm:ss", Locale.CHINA);
	/**
	 * 被格式化的时间对象，在被格式化之前，应该通过setTime()重新调整时间
	 */
	Date date = new Date();
	
	
}
